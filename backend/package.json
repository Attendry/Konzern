{
  "name": "konzern-backend",
  "version": "0.0.1",
  "description": "Backend für Konzern - Konsolidierte Jahresabschlüsse",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nest build",
    "postbuild": "node -e \"const fs=require('fs'); console.log('Checking dist/...'); if(!fs.existsSync('dist')){console.error('❌ dist/ directory does not exist'); process.exit(1);} const files=fs.readdirSync('dist'); console.log('Files in dist:', files.join(', ')); if(!fs.existsSync('dist/main.js')){console.error('❌ dist/main.js not found'); process.exit(1);} console.log('✅ Build successful: dist/main.js exists');\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^11.1.11",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/uuid": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "exceljs": "^4.4.0",
    "helmet": "^8.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "typescript-eslint": "^8.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "@eslint/js": "^9.39.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
